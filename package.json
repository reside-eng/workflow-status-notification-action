{
  "name": "@side/workflow-status-notification-action",
  "version": "0.0.0-development",
  "description": "Github Action workflow failure/success notification.",
  "main": "dist/index.js",
  "engines": {
    "node": "^16"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "build:docs": "ts-node bin/generate-docs.ts && prettier --write README.md",
    "build": "rimraf dist && ncc build src/index.ts && yarn build:docs",
    "lint": "eslint . --ignore-pattern='!.eslintrc.js' --ext .ts,.js",
    "types": "tsc --noEmit",
    "test": "export GITHUB_REPOSITORY='reside-eng/workflow-status-notification-action' && yarn tsc && jest"
  },
  "dependencies": {
    "@actions/cache": "1.0.11",
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/github": "5.1.1",
    "got": "11.8.6"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@side/commitlint-config": "0.1.12",
    "@side/eslint-config-base": "0.16.0",
    "@side/eslint-config-jest": "0.6.0",
    "@side/lint-staged-config": "0.1.0",
    "@side/prettier-config": "0.0.0",
    "@swc/core": "1.3.32",
    "@swc/jest": "0.2.24",
    "@tsconfig/node16": "1.0.3",
    "@types/jest": "27.5.2",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.18.12",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsdoc": "39.8.0",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "js-yaml": "4.1.0",
    "lint-staged": "13.1.1",
    "nock": "13.3.0",
    "prettier": "2.8.3",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "prettier": "@side/prettier-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/reside-eng/workflow-status-notification-action"
  },
  "keywords": [
    "actions",
    "workflow",
    "slack notification"
  ],
  "author": "Side",
  "license": "MIT",
  "private": false
}
