{
  "name": "@side/workflow-status-notification-action",
  "version": "0.0.0-development",
  "description": "Github Action workflow failure/success notification.",
  "main": "dist/index.js",
  "engines": {
    "node": "^20"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "build:docs": "tsx bin/generate-docs.ts && prettier --write README.md",
    "build": "rimraf dist && ncc build src/index.ts && yarn build:docs",
    "lint": "eslint . --ignore-pattern='!.eslintrc.js' --ext .ts,.js",
    "types": "tsc --noEmit",
    "test": "yarn tsc && GITHUB_REPOSITORY='reside-eng/workflow-status-notification-action' jest"
  },
  "dependencies": {
    "@actions/cache": "3.2.4",
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.0",
    "got": "11.8.6"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@side/commitlint-config": "1.0.2",
    "@side/eslint-config-base": "2.1.0",
    "@side/eslint-config-jest": "1.0.0",
    "@side/lint-staged-config": "1.0.2",
    "@side/prettier-config": "1.0.0",
    "@swc/core": "1.5.5",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/js-yaml": "4.0.9",
    "@types/node": "20.12.11",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "48.2.4",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "js-yaml": "4.1.0",
    "lint-staged": "15.2.2",
    "nock": "13.5.4",
    "prettier": "3.2.5",
    "rimraf": "5.0.7",
    "tsx": "4.10.0",
    "typescript": "5.4.5"
  },
  "prettier": "@side/prettier-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/reside-eng/workflow-status-notification-action"
  },
  "keywords": [
    "actions",
    "workflow",
    "slack notification"
  ],
  "author": "Side",
  "license": "MIT",
  "packageManager": "yarn@4.2.2",
  "private": true
}
